<Window x:Class="Amplicacion.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Amplicacion"
        mc:Ignorable="d"
        Title="MainWindow" Height="234" Width="437.6">


    <StackPanel Width="220" HorizontalAlignment="Left">
        <Grid Margin="5,80,0,0">
            <Grid.RowDefinitions>
                <RowDefinition Height="25"/>
                <RowDefinition Height="25"/>
                <RowDefinition Height="25"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="40"/>
                <ColumnDefinition Width="40"/>
                <ColumnDefinition Width="60"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <ListBox x:Name="ListParametros" HorizontalAlignment="Left" Height="50" Margin="0,-60,0,0" VerticalAlignment="Top" Width="70" SelectionChanged="ListBox_SelectionChanged_1" AllowDrop="True" Grid.ColumnSpan="2"/>
            <TextBox HorizontalAlignment="Left" Height="12" Margin="5,5,0,0" TextWrapping="Wrap" Text="Name:" VerticalAlignment="Top" Width="41" FontSize="8" Grid.Column="0" Grid.Row="0" BorderBrush="White"/>
            <TextBox HorizontalAlignment="Left" Height="12" Margin="0,5,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="35" FontSize="8" Grid.Column="1" Grid.Row="0"/>
            <TextBox HorizontalAlignment="Left" Height="12" Margin="5,5,0,0" TextWrapping="Wrap" Text="Epsilon:" VerticalAlignment="Top" Width="41" FontSize="8" Grid.Column="0" Grid.Row="1" BorderBrush="White"/>
            <TextBox HorizontalAlignment="Left" Height="12" Margin="0,5,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="35" FontSize="8" Grid.Column="1" Grid.Row="1"/>
            <TextBox HorizontalAlignment="Left" Height="12" Margin="5,5,0,0" TextWrapping="Wrap" Text="Delta:" VerticalAlignment="Top" Width="41" FontSize="8" Grid.Column="0" Grid.Row="2" BorderBrush="White"/>
            <TextBox HorizontalAlignment="Left" Height="12" Margin="0,5,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="35" FontSize="8" Grid.Column="1" Grid.Row="2"/>

            <TextBox HorizontalAlignment="Left" Height="12" Margin="5,5,0,0" TextWrapping="Wrap" Text="Alpha:" VerticalAlignment="Top" Width="50" FontSize="8" Grid.Column="2" Grid.Row="0" BorderBrush="White"/>
            <TextBox HorizontalAlignment="Left" Height="12" Margin="0,5,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="35" FontSize="8" Grid.Column="3" Grid.Row="0"/>
            <TextBox HorizontalAlignment="Left" Height="12" Margin="5,5,0,0" TextWrapping="Wrap" Text="Delta space:" VerticalAlignment="Top" Width="50" FontSize="8" Grid.Column="2" Grid.Row="1" BorderBrush="White"/>
            <TextBox HorizontalAlignment="Left" Height="12" Margin="0,5,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="35" FontSize="8" Grid.Column="3" Grid.Row="1"/>
            <TextBox HorizontalAlignment="Left" Height="12" Margin="5,5,0,0" TextWrapping="Wrap" Text="Delta time:" VerticalAlignment="Top" Width="50" FontSize="8" Grid.Column="2" Grid.Row="2" BorderBrush="White"/>
            <TextBox HorizontalAlignment="Left" Height="12" Margin="0,5,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="35" FontSize="8" Grid.Column="3" Grid.Row="2"/>
        </Grid>

    </StackPanel>
</Window>
